<script>
    window.onload = function() {
        var wrap = document.getElementById("wrap_area");
        var arrow = document.getElementById("arrow_bar");
        var slider = document.getElementById("slide_area");
        var lis = $(".slider-test");
        var Spans = $(".serverList span");
        var json = [
            {
            width: 550,
            top: 20,
            left: 50,
            opacity: 20,
            z: 5
        },
            {
                width: 600,
                top: 70,
                left: 0,
                opacity: 80,
                z: 8
            },
            {
                width: 800,
                top: 100,
                left: 200,
                opacity: 100,
                z: 10
            },
            {
                width: 600,
                top: 70,
                left: 600,
                opacity: 80,
                z: 8
            },
            {
                width: 500,
                top: 20,
                left: 750,
                opacity: 20,
                z: 4
            },
            {
                width: 450,
                top: 20,
                left: 650,
                opacity: 40,
                z: 2
            },
            {
                width: 500,
                top: 10,
                left: 100,
                opacity: 50,
                z: 0
            }];
        var jieliu = true;
        var as = arrow.children;
        change();
        for (var k in as) {
            as[k].onclick = function() {
                if (this.className == "prev_bar") {
                    if (jieliu == true) {
                        change(false);
                        jieliu = false
                    }
                } else {
                    if (jieliu == true) {
                        change(true);
                        jieliu = false
                    }
                }
            }
        }
        function change(flag) {
            if (flag) {
                json.unshift(json.pop())
            } else {
                json.push(json.shift())
            }
            for (var i = 0; i < json.length; i++) {
                animation(lis[i], {
                        width: json[i].width,
                        top: json[i].top,
                        left: json[i].left,
                        opacity: json[i].opacity,
                        zIndex: json[i].z
                    },
                    function() {
                        jieliu = true
                    })
            }
        }
        var timer = setInterval(change, 2500);
        wrap.onmouseover = function() {
            animation(arrow, {
                "opacity": 100
            });
            clearInterval(timer)
        };
        wrap.onmouseout = function() {
            animation(arrow, {
                "opacity": 0.5
            });
            timer = setInterval(change, 2500)
        };
        Spans.on("mouseenter",
            function() {
                clearInterval(timer)
            });
        Spans.on("mouseleave",
            function() {
                timer = setInterval(change, 2500)
            })
    };
    function animation(obj, json, fn) {
        clearInterval(obj.timer);
        obj.timer = setInterval(function() {
                var flag = true;
                for (var attr in json) {
                    var current = 0;
                    if (attr == "opacity") {
                        current = Math.round(parseInt(getStyle(obj, attr) * 100)) || 0.5
                    } else {
                        current = parseInt(getStyle(obj, attr))
                    }
                    var step = (json[attr] - current) / 10;
                    step = step > 0 ? Math.ceil(step) : Math.floor(step);
                    if (attr == "opacity") {
                        if ("opacity" in obj.style) {
                            obj.style.opacity = (current + step) / 100
                        } else {
                            obj.style.filter = "alpha(opacity = " + (current + step) * 10 + ")"
                        }
                    } else {
                        if (attr == "zIndex") {
                            obj.style.zIndex = json[attr]
                        } else {
                            obj.style[attr] = current + step + "px"
                        }
                    }
                    if (current != json[attr]) {
                        flag = false
                    }
                }
                if (flag) {
                    clearInterval(obj.timer);
                    if (fn) {
                        fn()
                    }
                }
            },
            10)
    }
    function getStyle(obj, attr) {
        if (obj.currentStyle) {
            return obj.currentStyle[attr]
        } else {
            return window.getComputedStyle(obj, null)[attr]
        }
    }
    setTimeout(function() {
            var serverlist = $(".serverList");
            serverlist.on("click",
                function() {
                    var lis = $(".slider-test");
                    $(this).find("span").css({
                        "color": "yellow"
                    }).parent("li").siblings("li").find("span").css({
                        "color": "#fff"
                    });
                    $(this).find(".circle-icon").css({
                        "backgroundColor": "#fff"
                    }).parent("li").siblings("li").find(".circle-icon").css({
                        "backgroundColor": "#7984a4"
                    });
                    var Index = $(this).index();
                    lis.eq(Index).css({
                        "width": 800,
                        "top": 100,
                        "left": 200,
                        "opacity": 100,
                        "zIndex": 100
                    });
                    lis.eq(Index).addClass("animated rollIn").siblings("li").removeClass("animated rollIn")
                })
        },
        80);
    var information = '${information}' == '' ? [] : JSON.parse('${information}');
    var str = '';
    for (var i in information) {
        var url = '/index/v2.5/home/news-details?id=' + information[i].id;
        var title = information[i].title;
        var createDate = new Date(information[i].createDate).Format('yyyy-MM-dd');
        str += '<li class="news news-spec"> ' +
            '<a href="'+ url +'">'+ title +'<br>' +
            '<span>'+ createDate +'</span> </a> </li>';
    }
    $('#newsList li:nth-of-type(1)').after(str)

</script>
