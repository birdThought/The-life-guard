package com.lifeshs.common.constants.common;

/**
 * 推送方式
 * author: wenxian.cai
 * date: 2017/8/24 11:08
 */
public enum  PushSendType {

    app(1, "app推送"),

    sms(2, "短信推送");

    private Integer value;

    private String remark;

    private PushSendType(Integer value, String remark) {
        this.value = value;
        this.remark = remark;
    }

    public Integer getValue() {
        return value;
    }

    public String getRemark() {
        return remark;
    }
}
