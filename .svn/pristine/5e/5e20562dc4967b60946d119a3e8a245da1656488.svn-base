*{
    margin: 0;
    padding: 0;box-sizing: border-box}
/* body {
    width: 1000px;
    margin: 0 auto;
    background: #f5f5f5;
} */
u,i,b,strong{
    text-decoration: none;
    font-style: normal;
}
.layui-form-radio>i:hover, .layui-form-radioed>i{
    color: #409eff;
}
.clearfix::before,
.clearfix::after{
    display: block;
    content: '';
    clear:both;
    visibility: hidden;
    height: 0;
}
.hide {
    display: none;
}
.db{
    display: block;
}
.dib {
    display: inline-block!important;
}
.vt{
    vertical-align: top;
}
.pl20{
    padding-left: 20px;
    padding-bottom: 4px;
}
.pt4{
    padding-top: 4px!important;
}
.ml6{
    margin-left: 6px;
}
.mr10 {
    margin-right: 10px;
}
.mb0{
    margin-bottom: 0px!important;
}
.mt0{
    margin-top: 0!important;
}
.mt8{
    margin-top: 8px;
}
.cup {
    cursor: pointer;
}
.w120{
    width: 120px!important;
}
.wauto {
    width: auto!important;
}
.layui-elem-field legend{
    font-size: 16px;
    color: #333;
}
.layui-elem-field legend > i {
    margin-right: 8px;
}
.article {
    width: 100%;
}
.content-header > h1 {
    font-size: 24px;
    font-weight: normal;
    line-height: 40px;
}
.content-body {
    background-color: #fff;
    width: 100%;
    padding: 20px;
}
.layui-elem-field{
    margin-top: 0!important;
}
.layui-form-label {
    font-size: 12px;
}
.layui-form-checked[lay-skin=primary] i {
    border-color: #409eff;
    background-color: #409eff;
    color: #fff;
}
.select-items {
    color: #409eff;
    font-size: 12px;
}
.select-options {
    margin-top: 7px;
}
.select-items > span:nth-child(1){
    color: #333;
    margin-right: 10px;
}
.select-tips {
    color: #A37F7F;
    font-size: 12px;
    margin-top: 5px;
}
.select-tips.spec {
    float: left;
    
}
.layui-form-select .layui-input {
    font-size: 12px;
}
.attr-select >div.layui-form-item {
    width: 200px;
}
.attr-select >div.layui-form-item.spec{
    width: 170px;
}
.layui-input, .layui-select, .layui-textarea{
    height: 32px;
}
.layui-form-label {
    padding: 6px 10px;
    width: 84px;
}
.layui-input-block.spec {
    margin-left: 84px;
}
.layui-input-block.specially {
    margin-left: 84px;
}
.add-attr-box{
    border-top: 1px solid #e7e7e7;
    padding: 16px 0 10px;
}
.shop-attr {
    width: 900px;
    float: left;
    border: 1px solid #e7e7e7;
    background: #f4f5f9;
    margin-top: 10px;
    padding-top: 12px;
    margin-bottom: 16px;
}
.add-attr {
    color: #409eff;
    font-size: 12px;
    float: left;
    line-height: 32px;
    border-top: 1px solid #e7e7e7;
    padding-left: 20px;
}
#add_attr{width: 100px;}
.layui-form-mid{
    line-height: 14px;
    font-size: 12px;
}
.layui-form-select dl dd, .layui-form-select dl dt,.layui-form-radio *,.layui-input, .layui-select, .layui-textarea{
    font-size: 12px;
}
.shop-attr  .layui-form-checkbox[lay-skin=primary]{
    margin-top: 0px;
    padding-left: 22px;
}
.CheckBox .layui-unselect{
    margin-bottom: 7px;
}
.CheckBox .layui-unselect > span {
    min-width: 68px;
    max-width: 100px;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    font-size: 12px;
    margin-right: 20px;
}
.layui-form-checkbox[lay-skin=primary] i{
    height: 12px;
    width: 12px;
    line-height: 12px;
}
.layui-form-radio{
    margin-top: 0;
}
.layui-form-radio>i{
    font-size: 17px;
}
.upload-pic {
    padding: 6px 15px;
}
.upload-pic-des{
    margin-bottom: 6px;
}
.layui-upload{
    padding-left: 96px;
}
.area-show,.area-show > i {
    font-size: 12px;
    color: purple;
}
.edit-area {
    color: #409eff;
    font-size: 12px;
    margin-left: 10px;
    cursor: pointer;
}
.putaway-tips{
    font-size: 12px;
    position: absolute;
    left: 86px;
    top: 4px;
    cursor: auto;
}
.putaway-tips > i {
    color: #409eff;
    font-weight: bold;
}
.putaway-tips:hover{
    color: #333;
}
.putaway-tips.spec {
    color: #e15f63;
    top: 34px;
    left: 80px;
}
.putaway-tips.spec:hover{
    color: #e15f63;
}
#popup .layui-input-inline {
    width: 120px;
}
#popup_f {
    margin-top: 20px;
}
#popup_f .layui-input-inline{
    width: 22%;
}
#popup_f .set-items {
    margin-left: 60px;
}
.layui-layer-page .layui-layer-content{
    overflow: visible!important;
}
.area-change-set{
    text-align: center;
    margin-top: 26px;
}
.set-items-icon {
    position: absolute;
    right: 6px;
    top: 8px;
}
#popup_f .layui-form-select .layui-edge{
    display: none;
}
.multi-specification{
    margin-left: 82px;
}
.multi-specification > div {
    border-bottom: 1px solid #e7e7e7;
}
.multi-specification-item-spec{
    border-bottom: none !important;
}
.multi-specification-title {
    font-size: 12px;
    margin-bottom: 8px;
}
.add-specification{
    font-size: 12px;
    color: #576BAF;
    margin-left: 20px;
    cursor: pointer;
}
.multi-specification-title>span {
    margin-right: 10px;
    cursor: pointer;
    color: #976BAF;
}
#rules_s .layui-form-label {
    width: 48px;
    padding: 6px 10px 6px 0;
}
#price_multi {
    background-color: #44c660;
    color: #eee;
}
.standard-area {
    border:1px solid #e7e7e7;
    /* width:75%; */
    width:55rem;
}
.standard-area tr{
    color: #333;
    font-size:13px;
    width:100%;
    box-sizing:border-box;
}
table.standard-area td{
	border: 1px solid #ddd
}
/* tr.skus-data > td > span {
	width:100%;
} */
tr.skus-data > td > span > input{
	width:100%;
	padding:4px 0 4px 0;
	border:none;
}
.standard-area > ul li {
    float: left;
    padding-left: 10px;
    line-height: 36px;
    background-color: #f4f5f9;
    border-bottom: 1px solid #e7e7e7;
}
th.table_cell_t,table_cell_data{
	width:6%;
	padding:6px 0 6px 0;
}
.standard-area > ul li:nth-child(1){
    width:22%;
}
.standard-area > ul li:nth-child(2){
    width:8%;
}
.standard-area > ul li:nth-child(3){
    width:15%;
}
.standard-area > ul li:nth-child(4){
    width:15%;
}
.standard-area > ul li:nth-child(5){
    width:14%;
    padding-left: 5px;
}
.standard-area > ul li:nth-child(6){
    width:26%;
    padding-left: 4px;
}
#standard_list {
    background-color: #fff;
}
#standard_list > div {
    display: flex;
    flex-wrap:nowrap;
    flex-direction:row;
    justify-content: flex-start;
}
#standard_list > div > div{
    padding:10px 20px;
}
#standard_list > div > div:nth-child(1){
    border-bottom: 1px solid #e7e7e7;
}
#standard_list > div > div:nth-child(1){
    width:25%;
    border-right:1px solid #e7e7e7;
}
#standard_list > div:nth-child(1) > div:nth-child(2){
    border-bottom: 1px solid #e7e7e7;
}
#standard_list > div {
    width:800px;
}
#standard_list > div > div ol li {
    width:100%;
}
#standard_list > div > div ol li span{
    display: inline-block;
    width:9%;
    padding-left: 12px;
}
#standard_list > div > div ol li input {
    width:17%;
    margin-right: 10px;
    border: 1px solid #e7e7e7;
    height: 24px;
    margin-bottom: 12px;
}
#standard_list > div > div ol li input:nth-of-type(1){
    width: 18%;
}
#standard_list > div > div ol li input:last-of-type{
    width: 28%;
}
#standard_list > div > div ol li input:last-child {
    margin-right:0;
}
.layui-layer-lan .layui-layer-btn{
    background-color: #fff;
}
#other-pictures div.uploaded-imgs{
	margin-left: 60px;
	padding:0 30px;
}
#other-pictures div.uploaded-imgs > span > img{
	width: 150px;height: 100px;
	padding-right:10px;
}

.table_cell_data > span > input{
	border:none;
}
.layui-form-select dl dd[lay-value='-1']{color:#79BAD5;}

span.confirm-add-attr-value{cursor: pointer;}









