/* header */
header{
	min-width: 1200px;
	background-color:#f8f8f8 ;
	height: 100px;
	line-height: 100px;
	box-shadow: 0 0 8px rgba(0, 0, 0,.4)
}
header ul li{
	display: inline-block;
	margin-right: 30px;
}
header ul li a{
	display: inline-block;
	font-size: 20px;
}
header ul li a.on{
	color: #369239;
}
header ul li a:hover{
	color: #369239;
}
.register a{
	color: #fff;
	width: 76px;
	height: 34px;
	line-height: 34px;
	text-align: center;
	border-radius: 6px;
	background-color: #369239;
}
.register a:hover{
	color: #fff;
}
.login a{
	color:#369239;
	border: 1px solid #369239;
	border-radius: 6px;
	text-align: center;
	line-height: 32px;
	height: 32px;
	width: 74px;
}
/* header */
/* content */
/* 导航条下拉框 */
ul.navbar-nav > li{
	position: relative;
}
ul.navbar-nav li ul {
    display: none;
}

ul.navbar-nav li:hover ul {
    position: absolute;
    display: block;
    line-height: 40px;
    z-index: 1000;
    left: -12px;
    top: 100px;
    padding: 5px 0;
    background: #f8f8f8;
    border-top: 2px solid #369239;
}
ul.navbar-nav li ul li {
    margin: 0 10px;
    border-bottom: 1px solid #d0d0d0;
}
 ul.navbar-nav li ul li:first-child {
    margin: 0 10px;
    border-top: 0 none;
}
ul.navbar-nav li ul li:last-child {
    border-bottom: 0 none;
}
ul.navbar-nav li ul li a, ul.navbar-nav li ul li a:link {
    color: #777;
    display: block;
    background: transparent none;
    padding: 10px 20px 10px 5px;
    white-space: nowrap;
}
ul.navbar-nav li ul li a:hover {
    text-decoration: underline;
    color:#369239 ;
}
/* 导航条下拉框 */
/* content */
/* footer */
footer{
	min-width: 1200px;
	background-color: #202226;
	color: #fff;
	padding-top: 40px;
	padding-bottom: 20px;
	font-size: 16px;
}
footer a{
	color: #ddd;
}
footer ul li,footer dl{
	display: inline-block;
	vertical-align: top;
	box-sizing: border-box;
}
footer ul li{
	padding-top: 10px;
	width: 24%;
}
footer ul li:nth-of-type(3){
	text-align: center;
}
footer ul li:nth-of-type(3) h2{
	font-size: 28px;
	font-weight:500;
}
footer ul li:nth-of-type(3) h2:hover{
	color:#ffb871;
}
footer ul li:nth-of-type(3) p{
	font-size: 16px;
	line-height: 2.2;
}
footer dl dt{
	font-size: 24px;
	font-weight: 500;
}

footer dl dd{
	line-height: 2.2
}
footer dl dd a:hover{
	color:#ffb871;
}
footer dl .offset-bottom{
	margin-top: 10px;
}
footer ul li:last-child{
	text-align: center;
}
footer ul li:last-child h4{
	margin-top: 10px;
	font-weight: 500;
	letter-spacing: 2px;
}

/* footer */


