/* QQ客服 */
.extension{
	position: fixed;
	right: 2%;
	top: 60%;
	z-index:999999;
}

.extension li{
	text-align:center;
	width:150px;
	margin-bottom:10px;
	background-color:#fff;
	border:1px solid #e0e0e0;
	
}
.extension li:first-child{
	padding-top:4px;
	padding-bottom:14px;
	position:relative;
}
.extension li:first-child img{
	width:124px;
}
.extension li:first-child p{
	color:#4c4c4c;
	font-size:14px;
	margin-bottom:4px;
}
.extension li:first-child input{
	width:7px;
	height:auto;
	position:absolute;
	top:5px;
	right:5px;
}
.extension li a{
	color: #4c4c4c;
    font-size:14px;
}
.extension li a p{
	padding-top:4px;
	padding-bottom:4px;
}
.extension li .qq_icon{
	background:url(../../../images/qq_icon.png) no-repeat 16px center;
}

