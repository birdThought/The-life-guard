package com.lifeshs.po.user;

import java.util.Date;

import lombok.Data;

/**
 *  用户报告分析实体
 *  @author yuhang.weng
 *  @version 1.0
 *  @DateTime 2017年10月9日 下午2:58:20
 */
@Data
public class ReportAnalysisPO {

    private Integer id;
    /** 用户id */
    private Integer userId;
    /** 请求者类型，1_会员，2_渠道商 */
    private Integer requestorType;
    /** 健康设备类型 */
    private Integer healthProduct;
    /** 分析报告内容 */
    private String content;
    /** 请求者阅读状态，0_未读，1_已读 */
    private Boolean read;
    /** 分析报告回复 */
    private String reply;
    /** 回复者（客服）id */
    private Integer replyUserId;
    /** 医生署名 */
    private String doctorSign;
    /** 状态：0正常，1异常 */
    private Integer status;
    /** 创建日期 */
    private Date createDate;
    /** 修改日期 */
    private Date modifyDate;
    /** 是否已删除 */
    private Boolean deleted;
}
