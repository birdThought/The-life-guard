package com.lifeshs.pojo.org.management;

/**
 *  机构树VO
 *  @author yuhang.weng  
 *  @DateTime 2016年9月7日 下午5:23:14
 */
public class OrgTreeVO {

	private Integer id;
	/** 机构名称 */
	private String orgName;
	/** 机构类型 0_管理机构、1_服务机构（门店） */
	private Integer type;
	/** 子级机构数量 */
	private Integer amountOfChildOrg;
	/** 子级机构会员数 */
	private Integer amountOfChildMember;
	/** 联系人名字 */
	private String contact;
	/** 联系号码 */
	private String contactInformation;
	
	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getOrgName() {
		return orgName;
	}

	public void setOrgName(String orgName) {
		this.orgName = orgName;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public Integer getAmountOfChildOrg() {
		return amountOfChildOrg;
	}

	public void setAmountOfChildOrg(Integer amountOfChildOrg) {
		this.amountOfChildOrg = amountOfChildOrg;
	}

	public Integer getAmountOfChildMember() {
		return amountOfChildMember;
	}

	public void setAmountOfChildMember(Integer amountOfChildMember) {
		this.amountOfChildMember = amountOfChildMember;
	}

	public String getContact() {
		return contact;
	}

	public void setContact(String contact) {
		this.contact = contact;
	}

	public String getContactInformation() {
		return contactInformation;
	}

	public void setContactInformation(String contactInformation) {
		this.contactInformation = contactInformation;
	}
}
