

.navbar .active {
    border-bottom: 2px solid #f40;
    color: #ff4400;
}
.content-top {
    margin-top:3rem;
}
.content-top p {
    font-size:1.4rem;
}
.content-top p span {
    color: #ff4400;
}
.content-bottom-title h1 {
    text-align: center;
    color: #333;
    font-size:2.4rem;
    line-height:10rem;
}
.content-bottom-middle {
    background-color: #f4f4f4;
    padding:3rem;
    padding-left: 3.9rem;
    font-size:1.4rem;
}
.content-bottom-middle .row {
    margin:0;
}
.content-bottom-middle > p {
    font-size:1.4rem;
}
.icon-lingxing {
    font-size:1rem;
    color: #44c660;
    margin-right: 0.5rem;
}
.middle-main-content{
    height:18rem;
    border:1px solid #ccc;
    border-right:none;
}
.middle-main-content > div {
    height:100%;
}
.middle-main-content > div dt {
    margin-bottom:1rem;

}
.middle-main-content > div dt,
.middle-main-content > div dd {
    line-height:1.7rem;
    font-size:1.4rem;
}
.middle-main-content > div dd {
    text-align: left;
    padding-left: 5%;
    margin-bottom:0.2rem;
}
.middle-fist > li:nth-of-type(1) {
    color: #333;
}
.middle-fist > li:nth-of-type(1) h5{
    font-size:1.6rem;
    line-height:2.2rem;
    font-weight: bold;
}
.middle-fist > li:nth-child(2) {
    margin-top:2.5rem;
}
.middle-fist > li:nth-of-type(2) p {
    font-size:1.6rem;
    line-height:2.6rem;
    margin-bottom:1rem;
}
.content-bottom-middle li:nth-of-type(1) p {
    font-size:1.4rem;
    line-height:2.4rem;
    letter-spacing: 1px;
}
.content-bottom-middle li:nth-of-type(2) div {
    width:25%;
    float: left;
    border-right: 1px solid #ccc;
    text-align: center;
}
.content-bottom-middle li:nth-of-type(2) div:nth-of-type(2){
    width:22%;
}
.content-bottom-middle li:nth-of-type(2) div:nth-of-type(3){
    width:53%;
}
.content-bottom-middle li:nth-of-type(2) div:nth-of-type(3) dd {
    padding-left: 2%;
    padding-right: 5%;
    border-right: none;
 }

.content-bottom-middle li:nth-of-type(2) div dl dt {
    background-color: #44c660;
    color: #fff;
    line-height:2.5rem;
}
.middle-fist > li:nth-of-type(3) {
    margin-top:3rem;
}
.middle-fist > li:nth-of-type(3) > h2 {
    font-size:1.8rem;
    color: #666;
    line-height:3.2rem;
    font-weight: bold;
}
.service-list {
    width:100%;
    height:auto;
    background: #fff;
    padding-top: 1%;
    padding-bottom: 1rem;
}
.service-list div ul li {
    font-size:1.4rem;
}
.service-list > div {
    display: inline-block;
}
.service-list > div:nth-child(1){
    width:50%;
    padding-left: 2%;
}
.service-list > div:nth-child(2){
    width:30%;
    vertical-align: top;
}
.service-list > div li {
    line-height:2rem;
}
.content-top p {
    font-size:1.5rem;
}

.teamwork {
    margin-top:80px;
}
.teamwork h4 {
    font-size:2.5rem;
    color: #5f6637;
    margin-bottom:2.4rem;
}

.teamwork p {
    font-size:1rem;
    color: #5f6637;
}
.linkfriends {
    margin-top:4rem;
}
@media screen and (max-width: 850px){
    .tablistbox {
        overflow: scroll;
    }
    .nav-pills {
        width:800px;
    }
    .tab-content {
        padding-left: 0;
    }
    .linkfriends li {
        min-height: 6rem !important;
    }

}


!*******************banner***************!
.banner {
    text-align: center;
    margin-bottom: 16px;
}

!*********************璺緞瀵艰埅****************!
.breadcrumb {
    background-color: inherit;
    font-size: 16px;
    padding:0;
}
.breadcrumb > .active {
    color:#44c660;
}
.breadcrumb > li + li:before {
    content:"|";
    color: #333;
    padding:0 15px;
}


!*******content*********!

.tree-nav {
    margin-top: 32px;
}

.tree-nav ul li a {
   position: relative;
   color:#999;
   margin-bottom: 50px;
   font-size:1.6rem;
   padding-left:40px;
   display: block;
}
.tree-nav ul li a::after {
   content: "";
   position: absolute;
   display: block;
   top:-5px;
   width: 30px;
   height: 30px;
   left:0;
}
.tree-nav ul li.company a::after {
   content: "";
   background-image: url(./../images/contact.jpg);
   vertical-align: middle;
}
.tree-nav ul li.partner a::after {
   content: "";
   background-image: url(./../images/partner.jpg);
   vertical-align: middle;
}
.tree-nav ul li.us a::after {
   content: "";
   background-image: url(./../images/call.jpg);
   vertical-align: middle;
}
.tree-nav ul li.recruit a::after {
   content: "";
   background-image: url(./../images/recruit.jpg);
   vertical-align: middle;
}
.tree-nav ul li.load a::after {
   content: "";
   background-image: url(./../images/load.jpg);
   vertical-align: middle;
}


.qrcode ul li {
   display: inline-block;
   margin:10px 75px 10px 0;
   text-align: center;
}

.qrcode ul li span {
   display: block;
   width:36px;
   height:36px;
   margin:25px 0 10px 92px ;
}
.message,.qrcode {
   padding-top: 2rem;
   padding-left: 7.4rem;
   border-left: 1px solid #ccc;
}
.message {
   padding-bottom: 5rem;
}
.message ul {
   padding:0;
}
.breadcrumb li {
   font-size:1.5rem;
}
.breadcrumb a {
   font-size:1.5rem;
}
.message h3 {
   font-size: 20px;
   color: #44c660;
}
.message ul li {
   font-size: 20px;
   line-height: 1.5;
   margin-top:34px;
}
.message h4 {
   font-size: 20px;
   margin-top:86px;
   color: #44c660;
}

.icon-xiazai {
   display: block;
   font-size:36px;
}
@media screen and (min-width:650px) and (max-width:1400px) {
    /* CSS 代码 */ .middle-main-content {
    height:auto;
}
}
@media screen and (max-width: 650px) {
   .middle-main-content {
       height:auto;
   }
   .content-bottom-middle li:nth-of-type(2) div{
       width:100%!important;
       height: auto;
       border-right: none;
   }
   .service-list > div {
       width:100% !important;
       padding-left: 4% !important;
   }
   .middle-main-content div dl dd {
       border-bottom: 1px dashed #ccc;
   }
   .QRcode-content {
       padding-left: 0;
       text-align: center;
   }
   .qrcode ul li {
       display: inline-block;
       margin:0;
       margin-bottom:1.2rem;
       text-align: center;
   }
   .content-bottom-middle {
       background-color: #f4f4f4;
       padding: 1rem;
       font-size: 1.4rem;
   }
   .content-bottom-middle li:nth-of-type(2) div:nth-of-type(3) dd {
       padding-left:5%;
   }
}

.route {
