/**
 * 分析报告
 */
.orderCenter {
    padding: 14px 20px 20px 20px !important;
    border: 1px solid #ddd;
    background: #fff;
    margin-top: 30px
}
.orderCenterTop {
    width:100%;
    /*margin-top:34px;*/
    padding-left: 24px;
}
.orderCenterTop  li {
    float: left;
    /*margin-right:50px;*/
}
.orderCenterTop  li:last-child {
    float: right;
    width:382px;
    height:auto;
}
.orderCenterTop  li > select{
    width: 94px;
    height:32px;
    border:1px solid #edd;
    padding-left: 12px;
    /*cursor: s-resize;*/
}
.search > input {
    float: left;
    width:302px;
    height:30px;
    border:1px solid #edd;
    text-align: center;
    color: #ccc;
    background: linear-gradient(#ccc 0% ,#fff 15% ,#fff 100%);
    padding-left: 20px;
}
.search > span {
    float: left;
    background: #3DB9FF;
    width:78px;
    height:30px;
    position: relative;
    color: #fff;
    padding-left: 35px;
    line-height:30px;
    cursor: pointer;
}
.search > span > i {
    position: absolute;
    width:14px;
    height:14px;
    background: url("/static/images/select-icon.png") -52px -10px;
    left:10px;
    top:10px;
}
.orderCenterBottom {
    margin-top:20px;

}
.orderCenterBottom ul >li:last-child {
    border-right: 1px solid #ddd;
}
.orderCenterBottom table {
    width:100%;
    text-align: center;
    border-collapse:collapse;
    position: relative;
}
.orderCenterBottom table td {
    border:1px solid #ddd;
    height:50px;
}
.orderCenterBottom table tr:nth-child(2n){
    background: #f8f8f8;
}
.orderCenterBottom table tr:hover {
    background: #f8f8f8;
}
.orderCenterBottom  thead  td {
    background:#f1f1f1;
    height:50px;
}
.currentColor {
    color: deepskyblue;
    cursor: pointer;
}
.layer-report {
}
.layui-layer-title {
    height: auto;
    background: #3cbaff!important;
    margin-bottom:30px;
    padding: 0!important;
}
.layui-layer-title > p{
    line-height: 48px!important;
    color: #fff!important;
    font-size: 16px!important;
}
#orderCenterPopup,#orderCenterPopup1,#orderCenterPopup2,#orderCenterPopup3,#orderCenterPopup4 {
    width:100%;
    height:100%;
    padding-left: 34px;
    display: none;
}
#orderCenterPopup section:nth-of-type(1) > p,#orderCenterPopup1 section:nth-of-type(1) > p,#orderCenterPopup2 section:nth-of-type(1) > p,#orderCenterPopup4 section:nth-of-type(1) > p,
{
    position: relative;
    color: #555;
    letter-spacing:1.5px;
}
#orderCenterPopup section:nth-of-type(1) >p::after,#orderCenterPopup1 section:nth-of-type(1) >p::after,#orderCenterPopup2 section:nth-of-type(1) >p::after,#orderCenterPopup4 section:nth-of-type(1) >p::after {
    width:54px;
    height:22px;
    position: absolute;
    content: '正常';
    background: #39BAFE;
    line-height:22px;
    text-align: center;
    color: #fff;
    left:115px;
    top:0;
    border-radius: 11px;
}
#orderCenterPopup section:nth-of-type(1) >p.unusual::after,#orderCenterPopup1 section:nth-of-type(1) >p.unusual::after,#orderCenterPopup2 section:nth-of-type(1) >p.unusual::after,#orderCenterPopup4 section:nth-of-type(1) >p.unusual::after {
    content: '异常';
    background: #ff2625;
}
#orderCenterPopup section:nth-of-type(2),#orderCenterPopup1 section:nth-of-type(2),#orderCenterPopup2 section:nth-of-type(2),#orderCenterPopup3 section:nth-of-type(2),#orderCenterPopup4 section:nth-of-type(2)  {
    margin-left:35px;
    margin-top:10px;
}
#orderCenterPopup section:nth-of-type(2) ul,#orderCenterPopup2 section:nth-of-type(2) ul,#orderCenterPopup3 section:nth-of-type(2) ul,#orderCenterPopup4 section:nth-of-type(2) ul {
    float: left;
    width:160px;
}
#orderCenterPopup1 section:nth-of-type(2) ul{
    width: 240px;
}
#orderCenterPopup section:nth-of-type(2) ul li,#orderCenterPopup1 section:nth-of-type(2) ul li,#orderCenterPopup2 section:nth-of-type(2) ul li,#orderCenterPopup3 section:nth-of-type(2) ul li,#orderCenterPopup4 section:nth-of-type(2) ul li {
    width:100%;
    line-height:40px;
    color: #222;
    font-weight: bold;
}
#orderCenterPopup section.reply,#orderCenterPopup1 section.reply,#orderCenterPopup2 section.reply,#orderCenterPopup3 section.reply,#orderCenterPopup4 section.reply{
    padding-top: 20px;
}
#orderCenterPopup section.reply > span,#orderCenterPopup1 section.reply > span,#orderCenterPopup2 section.reply > span,#orderCenterPopup3 section.reply > span,#orderCenterPopup4 section.reply > span {
    color: deepskyblue;
    margin-right:4px;
    vertical-align: top;
}
#orderCenterPopup section.reply > textarea,#orderCenterPopup1 section.reply > textarea,#orderCenterPopup2 section.reply > textarea,#orderCenterPopup3 section.reply > textarea,#orderCenterPopup4 section.reply > textarea {
    resize: none;
    border:1px solid #ddd;
    width: 442px;
    text-align: left;
    padding:10px;
}

.ny {
    color: deepskyblue;
    cursor: pointer;
}
.ny:hover {
    border-bottom: 1px dashed deepskyblue;
}
.doctor-sign {
    border: 1px solid #ddd;
    width: 442px;
    padding: 10px;
}
.titleShow {
    padding-bottom: 10px;
    margin-bottom: 6px;
    border-bottom: 1px dashed #ccc;
}
textarea#content {
   height: 350px;
    width: 750px;
    padding: 10px;
    margin-left: 5px;
    resize: none;
    border-radius: 10px;
    box-shadow: 0px 0 20px #cdd;
    border: 1px solid #dee;
}
span.small-tip{
    border: 1px solid #ddd;
    border-right: none;
    line-height: 30px;
    padding: 0 4px;
    float: left;
    background: #f0f0f0;
    color: #455;
}
.Select-Options{
    float: left;
}
