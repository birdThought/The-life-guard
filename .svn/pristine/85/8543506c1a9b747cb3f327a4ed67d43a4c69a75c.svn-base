package com.lifeshs.common.constants.common;

/**
 *  广告类型
 *  @author yuhang.weng
 *  @version 1.0
 *  @DateTime 2017年4月12日 上午10:51:31
 */
public enum AdType {

    /** BANNER */
    BANNER(1, "Banner");
    
    private int value;
    
    private String remark;
    
    private AdType(int value, String remark) {
        this.value = value;
        this.remark = remark;
    }
    
    public int getValue() {
        return value;
    }
    
    public String getRemark() {
        return remark;
    }
}
