.health-consultAll{
	margin-bottom: 70px;
    padding: 20px;
    margin-top: 40px;
    box-shadow: -1px 5px 30px rgba(0, 0, 0, .1);
}
.health-consultAll ul{
    width: 80%;
    margin: 0 auto;
}
.health-consultAll li{
    display: inline-block;
    width: 42%;
}
.healthy-consult{
    overflow: hidden;
}
.healthy-consult div{
    float: left;
    margin-right: 2%;
    width: 20%;
}
.healthy-consult div img{
    width: 60px;
    height: 60px;
}
.healthy-consult dl {
    float: right;
    font-size: 16px;
    width: 78%;
}

/* 智能设备 */
.item-service{
    margin-top:20px;
    margin-bottom: 20px;
}
.item-service .item-subTiletle h3{
    border-left:3px solid #48c858;
    padding-left:20px;
    font-size: 18px;
}
.item-service .item-subTiletle{
    height:50px;
}
.inteDevice li{
	display: inline-block;
    width: 190px;
    height: 240px;
    border: 1px solid #ddd;
    margin-right: 40px;
    margin-bottom: 40px;
    border-radius: 10px;
    text-align: center;
    background-color:#fafafa;
}
.inteDevice li img{
	width: 120px;
	height: 120px;
	margin:20px auto;
}
.inteDevice li .check{
	cursor: pointer;
    width: 80px;
    border: 1px solid #48c858;
    border-radius: 4px;
    padding:2px 0;
    margin: 4px auto;
    background: #48c858;
}
.inteDevice li .check a{
	color:#fff;
}