<script>
    common.activityMenu(5, 1);
    var lis = $('.service-main-content-bottom li');
    var $width = $(window).width();
    lis.click(function () {
        if($width<650){
            $(this).find('i').addClass('xingxing').parent('li').siblings('li').find('i').removeClass('xingxing')
            $(this).find('h3').css({'color':"blue"}).parent('li').siblings('li').find('h3').css({'background':'#333'})
        }else {
            $(this).css({'transform':'scale(1.1)','z-index':11,'transition':'all 0.5s'}).siblings('li').css({'transform':'scale(1)','z-index':10,'transition':'all 0.5s'})
            $(this).find('i').addClass('xingxing').parent('li').siblings('li').find('i').removeClass('xingxing');
            $(this).find('h3').css({'color':"#228de8"}).parent('li').siblings('li').find('h3').css({'color':'#333'})
        }
    });
    var ins = {};
    ins.id = 115;
    ins.name = '血糖仪说明书';
    ins.changeIns = function (value) {
        switch (value) {
            case 'bloodSugar':
                ins.id = 115;
                ins.name = '| 血糖仪说明书';
                break;
            case 'band':
                ins.id = 119;
                ins.name = '｜心率手环说明书';
                break;
            case 'lunginstrument':
                ins.id = 118;
                ins.name = '｜肺活仪说明书';
                break;
            case 'bloodPressure':
                ins.id = 105;
                ins.name = '｜血压计操作说明';
                break;
            case 'bodyFatScale':
                ins.id = 104;
                ins.name = '｜体脂秤操作说明';
                break;
            case 'oxygen':
                ins.id = 103;
                ins.name = '｜血氧仪操作说明';
                break;
        }
        ins.getIns();
    };
    ins.getIns = function () {
        var url = '/informationControl.do?information&id=' + ins.id;
        $.ajax({
            type : 'GET',
            contentType : 'application/x-www-form-urlencoded; charset=utf-8',
            url : url,
            data : null,
            success : function(result) {
                if (result.success) {
                    $('.ins-bloodsugar').html('');
                    $('.ins-bloodsugar').append(result.obj);
                    $('.ins-bloodsugar table').css('width', '100%');
                    $('.ins-bloodsugar img').css('width', '100%');
                    $('.ins-device-name').text(ins.name);
                }
           
            }
        });
    };
    ins.changeIns('bloodSugar');
</script>
<script>
    /*说明书分页代码：*/
    var _offset = 0;
    function _$id(_obj) {
        return document.getElementById(_obj);
    }
    function _$Pages(_i) {
        _$id("content").scrollTop = (_$id("content").clientHeight*_i);
        ab=document.getElementById("pages");
        aa=ab.getElementsByTagName("a").length;
        for(k=0;k<aa;k++){
            if(k==_i){ab.getElementsByTagName("a")[k].style.color="red";
            }else{
                ab.getElementsByTagName("a")[k].style.color="#000000"
            }
        }
    }
    function _$InitPages() {
        var _s='';
        _offset = _$id("content").clientHeight;
        var _top = _$id("content").scrollTop;
        for (var i=0;i<(_$id("content").scrollHeight/_offset);i++) {
            _s+=('<a href="javascript:void(0);" onclick="javascript:_$Pages('+i+');" onfocus="this.blur()" title="Page '+(i+1)+'">'+(i+1)+'</a>    ');
        }
        _$id("pages").innerHTML=_s;
    }

</script>