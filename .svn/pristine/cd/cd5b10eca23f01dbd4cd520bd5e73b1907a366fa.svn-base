package com.lifeshs.pojo.data;

import java.util.List;

/**
 * 测量点
 * 
 * @author yuhang.weng
 * @version 1.0
 * @DateTime 2017年5月3日 下午6:12:31
 */
public class MeasureSiteDTO {

    private Integer id;

    /** 名称 */
    private String name;

    /** 经度 */
    private Double longitude;

    /** 纬度 */
    private Double latitude;

    /** logo */
    private String logo;

    /** 详细页展示图片 */
    private String banner;

    /** 联系电话 */
    private String phoneNumber;

    /** 地址 */
    private String address;

    /** 开放时间 */
    private List<MeasureSiteOpeningTimeDTO> openingTimeList;

    /** 设备列表 */
    private List<MeasureSiteDeviceDTO> deviceList;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Double getLongitude() {
        return longitude;
    }

    public void setLongitude(Double longitude) {
        this.longitude = longitude;
    }

    public Double getLatitude() {
        return latitude;
    }

    public void setLatitude(Double latitude) {
        this.latitude = latitude;
    }

    public String getLogo() {
        return logo;
    }

    public void setLogo(String logo) {
        this.logo = logo;
    }

    public String getBanner() {
        return banner;
    }

    public void setBanner(String banner) {
        this.banner = banner;
    }

    public String getPhoneNumber() {
        return phoneNumber;
    }

    public void setPhoneNumber(String phoneNumber) {
        this.phoneNumber = phoneNumber;
    }

    public List<MeasureSiteOpeningTimeDTO> getOpeningTimeList() {
        return openingTimeList;
    }

    public void setOpeningTimeList(List<MeasureSiteOpeningTimeDTO> openingTimeList) {
        this.openingTimeList = openingTimeList;
    }

    public List<MeasureSiteDeviceDTO> getDeviceList() {
        return deviceList;
    }

    public void setDeviceList(List<MeasureSiteDeviceDTO> deviceList) {
        this.deviceList = deviceList;
    }

    public String getAddress() {
        return address;
    }

    public void setAddress(String address) {
        this.address = address;
    }

    @Override
    public String toString() {
        return "MeasureSiteDTO [id=" + id + ", name=" + name + ", longitude=" + longitude + ", latitude=" + latitude
                + ", logo=" + logo + ", banner=" + banner + ", phoneNumber=" + phoneNumber + ", address=" + address
                + ", openingTimeList=" + openingTimeList + ", deviceList=" + deviceList + "]";
    }

}
