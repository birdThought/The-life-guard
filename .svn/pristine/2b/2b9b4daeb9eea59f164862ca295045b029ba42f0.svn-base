package com.lifeshs.common.constants.common.project;

/**
 *  项目状态
 *  @author yuhang.weng
 *  @version 1.0
 *  @DateTime 2017年7月21日 上午10:40:34
 */
public enum Status {

    STANDBY(1, "待上线"),

    ONLINE(2, "已上线"),

    OFFLINE(3, "已下线");

    private int value;
    private String remark;

    private Status(int value, String remark) {
        this.value = value;
        this.remark = remark;
    }

    public int getValue() {
        return value;
    }

    public String getRemark() {
        return remark;
    }

}
