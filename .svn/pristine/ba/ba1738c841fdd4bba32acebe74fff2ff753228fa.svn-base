/*文件上传区域*/
.sl-custom-file {
    position: relative;
    display: inline-block;
    zoom: 1;
    cursor: pointer;
    overflow: hidden;
    vertical-align: middle;
}
.btn-file{
    background-color: rgb(245,245,245) !important;
    color: #fff !important;
    border: 1px solid #f5f5f5 !important;
    width: 90px !important;
    height: 90px !important;
    border-radius: 0 !important;
}
.sl-custom-file .ui-input-file {
    position: absolute;
    right: 0;
    top: 0;
    _zoom: 30;
    font-size: 300px;
    height: 100%;
    _height: auto;
    opacity: 0;
    filter: alpha(opacity=0);
    -ms-filter: "alpha(opacity=0)";
    cursor: pointer;
}
.upload-icon {
    position: absolute;
    width:28px;
    height:28px;
    background: url("../images/upload-icon.png") no-repeat center center;
    left:50%;
    top:50%;
    margin-top:-14px;
    margin-left:-14px;
}
input,button{outline:none}
select#selectPointOfInterest
{
    width                    : 140px;
    height                   : 30px;
    line-height              : 30px;
    padding-right            : 20pt;
    text-indent              : 4pt;
    text-align               : left;
    vertical-align           : middle;
    box-shadow               :  0 0 3px #606060;
    border                   : 1px solid #ddd;
    -webkit-appearance       : none;
    -moz-appearance          : none;
    appearance               : none;
    font-size                : 13px;
    color                    : #333;
    cursor                   : pointer;
    outline                  : none;
}
select#selectPointOfInterest option
{
    padding             : 4px 10px 4px 10px;
    font-size           : 11pt;
    font-weight         : normal;
}
select#selectPointOfInterest option[selected]{ font-weight:bold}
select#selectPointOfInterest option:nth-child(even) { background-color:#f5f5f5; }
/*LABEL FOR SELECT*/
label#lblSelect{ position: relative; display: inline-block;}
/*DOWNWARD ARROW (25bc)*/
label#lblSelect::after
{
    content                 : "\25bc";
    position                : absolute;
    height                  :29px;
    top                     : 1px;
    right                   : 1px;
    bottom                  : 0;
    width                   : 22px;
    line-height             : 29px;
    vertical-align          : middle;
    text-align              : center;
    background              : #E2E2E2;
    color                   : #000;
    pointer-events          : none;
}
/*文件上传区域结束*/
