package com.lifeshs.po;

import lombok.Data;

import java.util.Date;

/**
 * 订单-机构短信充值
 * author: wenxian.cai
 * date: 2017/8/28 17:23
 */
public @Data class OrderStoreSmsPO {

    /**id*/
    private int id;

    /**订单号*/
    private String orderNumber;

    /**交易标题/订单标题*/
    private String subject;

    /**交易的内容描述*/
    private String body;

    /**价格*/
    private int price;

    /**数量*/
    private int number;

    /**费用*/
    private int charge;

    /**状态：待付款_1, 付款失败_2，已完成_4*/
    private int status;

    /**机构id*/
    private int orgId;

    /**是否已删除(0：未删除；1：已删除)*/
    private int deleted;

    /**创建时间*/
    private Date createDate;

}
