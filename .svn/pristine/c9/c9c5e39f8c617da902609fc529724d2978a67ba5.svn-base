package com.lifeshs.pojo.org.group;

/**
 * 健康课堂群组机构用户
 * 
 * @author yuhang.weng
 * @version 1.0
 * @DateTime 2017年3月2日 上午11:12:23
 */
public class LessonGroupOrgUserDTO {

    /** 机构用户ID */
    private int id;
    /** 用户编号 */
    private String userCode;
    /** 头像 */
    private String photo;
    /** 显示名称 */
    private String realName;
    /** 机构用户类型 0_管理员,1_服务师,2_管理员&服务师 */
    private int orgUserType;
    /** 群组用户类型 0_群主,1_服务师 */
    private int groupUserType;
    /** 手机号 */
    private String mobile;
    /** 性别 */
    private Boolean sex;
    /** 邮箱 */
    private String email;
    /** 工作电话 */
    private String tel;
    /** 详细描述 */
    private String detail;
    /** 简介 */
    private String about;

    /** 获取 机构用户ID */
    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    /** 获取 用户编号 */
    public String getUserCode() {
        return userCode;
    }

    public void setUserCode(String userCode) {
        this.userCode = userCode;
    }

    /** 获取 头像 */
    public String getPhoto() {
        return photo;
    }

    public void setPhoto(String photo) {
        this.photo = photo;
    }

    /** 获取 显示名称 */
    public String getRealName() {
        return realName;
    }

    public void setRealName(String realName) {
        this.realName = realName;
    }

    /** 获取 机构用户类型 0_管理员,1_服务师,2_管理员&服务师 */
    public int getOrgUserType() {
        return orgUserType;
    }

    public void setOrgUserType(int orgUserType) {
        this.orgUserType = orgUserType;
    }

    /** 获取 群组用户类型 0_群主,1_服务师 */
    public int getGroupUserType() {
        return groupUserType;
    }

    public void setGroupUserType(int groupUserType) {
        this.groupUserType = groupUserType;
    }

    public String getMobile() {
        return mobile;
    }

    public void setMobile(String mobile) {
        this.mobile = mobile;
    }

    public Boolean getSex() {
        return sex;
    }

    public void setSex(Boolean sex) {
        this.sex = sex;
    }

    public String getEmail() {
        return email;
    }

    public void setEmail(String email) {
        this.email = email;
    }

    public String getTel() {
        return tel;
    }

    public void setTel(String tel) {
        this.tel = tel;
    }

    /** 获取 详细描述 */
    public String getDetail() {
        return detail;
    }

    public void setDetail(String detail) {
        this.detail = detail;
    }

    /** 获取 简介 */
    public String getAbout() {
        return about;
    }

    public void setAbout(String about) {
        this.about = about;
    }

    @Override
    public String toString() {
        return "LessonGroupOrgUserDTO [id=" + id + ", userCode=" + userCode + ", photo=" + photo + ", realName="
                + realName + ", orgUserType=" + orgUserType + ", groupUserType=" + groupUserType + ", mobile=" + mobile
                + ", sex=" + sex + ", email=" + email + ", tel=" + tel + "]";
    }

}
