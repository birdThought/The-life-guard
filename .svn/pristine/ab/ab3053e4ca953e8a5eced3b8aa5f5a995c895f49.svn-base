package com.lifeshs.pojo.record;

/**
 *  病历VO对象
 *  @author yuhang.weng  
 *  @DateTime 2016年9月19日 下午7:41:29
 */
public class MedicalVO {

	/** ID */
	private Integer id;
	/** 标题 */
	private String title;
	/** 用户名 */
	private String name;
	/** 性别 */
	private String sex;
	/** 年龄 */
	private Integer age;
	/** 基本病情 */
	private String basicCondition;
	/** 就诊日期 */
	private String visitingDate;
	/** 病历图片 */
	private String photo;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getSex() {
		return sex;
	}

	public void setSex(String sex) {
		this.sex = sex;
	}

	public Integer getAge() {
		return age;
	}

	public void setAge(Integer age) {
		this.age = age;
	}

	public String getBasicCondition() {
		return basicCondition;
	}

	public void setBasicCondition(String basicCondition) {
		this.basicCondition = basicCondition;
	}

	public String getVisitingDate() {
		return visitingDate;
	}

	public void setVisitingDate(String visitingDate) {
		this.visitingDate = visitingDate;
	}

	public String getPhoto() {
		return photo;
	}

	public void setPhoto(String photo) {
		this.photo = photo;
	}
}
