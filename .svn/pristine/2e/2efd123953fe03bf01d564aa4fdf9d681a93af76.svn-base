        -webkit-transform: scale(0) rotateX(360deg);
        transform: scale(0) rotateX(360deg);
        height: 150px;
        width: 150px;
    }

    50% {
        -moz-transform: none;
        -ms-transform: none;
        -webkit-transform: none;
        transform: none;
        height: 150px;
    }

    75% {
