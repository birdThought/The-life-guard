html, body {
    height: 100%;padding: 0;margin: 0
}

body {
    background: url("/static/platform/v2.2.0/images/hyt-login-bg.png") no-repeat center center;

}
.animated {
    animation-duration: 1.6s;
    animation-fill-mode: both;
}

.animated.infinite {
    animation-iteration-count: infinite;
}

.animated.hinge {
    animation-duration: 2s;
}

.animated.flipOutX,
.animated.flipOutY,
.animated.bounceIn,
.animated.bounceOut {
    animation-duration: .75s;
}
