/*
Name：移动页面首页css部分
CreateDate：2017/8/22
*/
html,body{width:100%;height:100%}
.M_banner {
    width: 100%;
    height:100%;
}
.M_header {
    height:auto;
    position: relative;
}
.M_header section:nth-child(1) {
    height:100%;
    text-align: center;
    width:100%;
}
.M_header section:nth-child(1) .M_header_img{
    min-width:100%;
    max-width:100%;
    height:100%;
}

.M_banner section:nth-of-type(1) {
    background: #fff;
    height:auto;
    width:100%;
}
.M_header section:nth-of-type(1) > a {
    position: absolute;
    top:3%;
    right:5%;
    width:6.5%;
    height:4.5%;
}
.M_header section:nth-of-type(1) > a > img {
    height:100%;
    width:100%;
}
.nav-top {
    text-align: center;
    height:25%;
    line-height: 0.7rem;
}
.M_banner section:nth-of-type(1) nav p:nth-of-type(1) {
    font-size:0.26rem;
    color: #999;
    letter-spacing: 0.03rem;
}
nav {
    height:100%;
}
.nav-bottom {
    height:75%;
    position: relative;
    text-align: center;
}
.nav-bottom h5 {
    width:100%;
    height:36%;
}
.nav-bottom h5 p {
    position: relative;
    left:50%;
    margin-left:-34.605%;
    width:69.21%;
    height:100%;
    text-align: center;
}
.nav-bottom h5:nth-of-type(1) input[type=button],
.nav-bottom h5:nth-of-type(2) input[type=button]
{
    width:100%;
    height:100%;
    border-radius: 45px;
    cursor: pointer;
    font-size:0.28rem;
    font-family:"fzlthjw--gb1-0";
}
.nav-bottom h5 > p img {
    height:48%;
    width:7.6%;
    position: absolute;
    left:17%;
    top:23%;
    z-index:999;
}
.nav-bottom h5:nth-of-type(2)  p img {
    top:57%;
    height:50%;
    width:7.6%;
}
.nav-bottom h5:nth-of-type(1) input[type=button] {
    background-color: #44c660;
    color: #fff;
    letter-spacing: 0.02rem;
}
.nav-bottom h5:nth-of-type(2) input[type=button] {
    background-color: #fff;
    color: #44c660;
    border:1px solid #44c660;
    margin-top:6%;
    letter-spacing: 0.025rem;
}