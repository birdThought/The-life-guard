package com.lifeshs.common.constants.common.jianKe;

public enum OrderStatusEnum {
    
    /** 未支付 */
    UNPAID(1, "未支付"),
    /** 已支付 */
    ALREADYPAID(2, "已支付"),
    /** 已完成 */
    COMPLETED(3, "已完成");
    
    private int value;
    /** 描述 */
    private String remark;

    private OrderStatusEnum(Integer value, String remark) {
        this.value = value;
    }

    public int getValue() {
        return this.value;
    }

    public String getRemark() {
        return this.remark;
    }
}
