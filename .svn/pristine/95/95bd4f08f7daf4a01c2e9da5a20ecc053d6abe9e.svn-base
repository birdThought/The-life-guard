}
#submitData > section:nth-of-type(2) >  ul > li,#submitData1 > section:nth-of-type(2) >  ul > li,#submitData2 > section:nth-of-type(2) >  ul > li,#submitData3 > section:nth-of-type(2) >  ul > li,#submitData4 > section:nth-of-type(2) >  ul > li{
