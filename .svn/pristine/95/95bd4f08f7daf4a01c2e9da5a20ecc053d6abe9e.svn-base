    padding-left:34px;
    display: none;
}
#submitData > section:nth-of-type(1),#submitData1 > section:nth-of-type(1),#submitData2 > section:nth-of-type(1),#submitData3 > section:nth-of-type(1),#submitData4 > section:nth-of-type(1) {
    padding-top: 30px;
}
#submitData > section:nth-of-type(1) > p,#submitData1 > section:nth-of-type(1) > p,#submitData2 > section:nth-of-type(1) > p,#submitData3 > section:nth-of-type(1) > p,#submitData4 > section:nth-of-type(1) > p{
